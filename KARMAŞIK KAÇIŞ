  onKeyPress(e) {
    if (e.keyCode === 37 && this.velocityX !== 1) {
      this.velocityX = -1;
      this.velocityY = 0;
    }
    
    else if (e.keyCode === 38 && this.velocityY !== 1) {
      this.velocityX = 0;
      this.velocityY = -1;
    }
    
    else if (e.keyCode === 39 && this.velocityX !== -1) {
      this.velocityX = 1;
      this.velocityY = 0;
    }
    
    if (e.keyCode === 40 && this.velocityY !== -1) {
      this.velocityX = 0
      this.velocityY = 1;
    }
    if (e.keyCode === 82 && this.velocityY !== -1) {
      window.location.reload(false);
    }
    if (e.keyCode === 78 && this.velocityY !== -1) {
      window.alert("NasÄ±l OynanÄ±r: Ok tuÅŸlarÄ± ile oynanÄ±r amaÃ§ 7 tane pembe kartÄ± toplayarak kaÃ§maktÄ±r. YeÅŸil adamlar oyununu en baÅŸtan baÅŸlatÄ±r. SarÄ±, mor ve kÄ±rmÄ±zÄ± adamlar sizi belirli yerlere Ä±ÅŸÄ±nlar. Beyaz adamlar ise sizi rastgele yerlere Ä±ÅŸÄ±nlamaktadÄ±r. !DÄ°KKAT EDÄ°N KOLAY DEÄžÄ°L!");
      window.location.reload(false);
    }
  }
}

const game = new SnakeGame();
  
window.onload = () => game.init();
</script>
